{
  "name": "mkulimalink",
  "version": "2.0.0",
  "description": "AI-powered agriculture super-app for East Africa - marketplace, finance, logistics, and community",
  "main": "server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "build": "npm run build --prefix frontend",
    "install-all": "npm install && cd frontend && npm install",
    "test": "jest --coverage",
    "lint": "eslint backend/**/*.js",
    "seed": "node backend/utils/seedData.js",
    "migrate": "node backend/utils/migrate.js"
  },
  "keywords": ["agriculture", "marketplace", "tanzania", "ai", "fintech", "agritech", "east-africa"],
  "author": "MkulimaLink Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.0",
    "mongoose": "^8.5.0",
    "dotenv": "^16.4.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.7.5",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.4.0",
    "sharp": "^0.33.5",
    "@tensorflow/tfjs-node": "^4.21.0",
    "africastalking": "^0.7.0",
    "cheerio": "^1.0.0",
    "node-cache": "^5.1.2",
    "socket.io": "^4.7.5",
    "redis": "^4.7.0",
    "ioredis": "^5.4.1",
    "bullmq": "^5.12.0",
    "winston": "^3.14.2",
    "morgan": "^1.10.0",
    "express-validator": "^7.2.0",
    "uuid": "^10.0.0",
    "qrcode": "^1.5.4",
    "pdf-lib": "^1.17.1",
    "xlsx": "^0.18.5",
    "geolib": "^3.3.4",
    "dayjs": "^1.11.13",
    "slugify": "^1.6.6",
    "sanitize-html": "^2.13.0",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.4",
    "hpp": "^0.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.9.0"
  }
}
